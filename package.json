{
  "name": "savvy.no",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "yarn && yarn doppler:setup",
    "dev": "doppler run -- next dev",
    "start": "doppler run -- next start",
    "build": "doppler run -- next build",
    "build:static": "next build && next export && next-export-optimize-images",
    "postbuild:static": "next-sitemap",
    "doppler:setup": "doppler setup --project 'savvy-no' --config 'dev'"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@types/node": "18.11.8",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "clsx": "^1.2.1",
    "next": "latest",
    "next-export-optimize-images": "3.2.0",
    "next-sitemap": "^3.1.30",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "sass": "^1.55.0"
  },
  "volta": {
    "node": "18.12.0",
    "yarn": "1.22.19"
  }
}
